SECRET_KEY=your-secure-secret-key-here
FLASK_APP=app.py
FLASK_ENV=development
# Local database
# DATABASE_URL=postgresql://postgres:admin@localhost:5432/postgres
# AWS RDS database - update with your actual endpoint after deployment
DATABASE_URL=postgresql://postgres:${DB_PASSWORD}@${DB_ENDPOINT}:5432/winstongallery
# AWS S3 bucket - update with your actual bucket name after deployment
S3_BUCKET=winstongallerystack-winstonbucket
S3_REGION=us-east-1
S3_URL_EXPIRATION=3600
# AWS Secrets Manager - update with your actual secret ARN after deployment
DB_SECRET_ARN=${DB_SECRET_ARN}
